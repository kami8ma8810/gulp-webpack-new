{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"sourceType": "module",
		"babelOptions": {
			"configFile": "./babel.config.js"
		}
	},
	"globals": {
		"jQuery": "readonly",
		"$": "readonly"
	},
	"rules": {
		"semi": [
			"warn",
			"always"
		]
	}
}